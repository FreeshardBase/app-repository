{
  "v": "1.0",
  "app_version": "1.8.8",
  "name": "docuseal",
  "pretty_name": "DocuSeal",
  "icon": "icon.svg",
  "upstream_repo": "https://github.com/docusealco/docuseal",
  "entrypoints": [
    {
      "container_name": "docuseal",
      "container_port": 3000,
      "entrypoint_port": "http"
    }
  ],
  "paths": {
    "": {
      "access": "private"
    },
    "/d/": {
      "access": "public"
    },
    "/s/": {
      "access": "public"
    },
    "/disk/": {
      "access": "public"
    },
    "/packs/": {
      "access": "public"
    },
    "/api/attachments": {
      "access": "public"
    },
    "/submitters/": {
      "access": "public"
    }
  },
  "lifecycle": {
    "always_on": false,
    "idle_time_for_shutdown": 3600
  },
  "store_info": {
    "description_short": "DocuSeal allows to create, fill, and sign digital documents.",
    "is_featured": true
  }
}
