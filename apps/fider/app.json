{
  "v": "3.0",
  "name": "fider",
  "image": "getfider/fider:stable",
  "port": 3000,
  "env_vars": {
    "DATABASE_URL": "{{ postgres.connection_string }}",
    "JWT_SECRET": "foobar",
    "EMAIL_NOREPLY": "foo@bar.com"
  },
  "services": [
    "postgres"
  ],
  "paths": {
    "": {
      "access": "public",
      "headers": null
    }
  },
  "store_info": {
    "description_short": "Fider",
    "description_long": [
      "Fider"
    ]
  }
}