version: '3.5'

networks:
    portal:
        external: true

services:
    filebrowser:
        restart: always
        image: portalapps.azurecr.io/ptl-apps/filebrowser:master
        container_name: filebrowser
        volumes:
        - "{{ fs.app_data }}:/data"
        - "{{ fs.shared }}:/data/shared"
        - "{{ fs.all_app_data }}:/data/app_data"
        networks:
        - portal
