{
  "name": "gitea",
  "description": "Gitea is a community managed lightweight code hosting solution written in Go.",
  "image": "gitea/gitea:1.16.0",
  "port": 3000,
  "data_dirs": [
    {
      "path": "/data",
      "uid": 1000,
      "gid": 1000
    }
  ],
  "env_vars": {
    "USER_UID": "1000",
    "USER_GID": "1000",
    "DOMAIN": "gitea.{{ portal.domain }}"
  }
}